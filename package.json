    "json-loader": "^0.5.4",
    "karma": "^1.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-webpack": "^1.7.0",
    "node-sass": "^4.5.0",
    "react-addons-test-utils": "^15.2.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.0",
